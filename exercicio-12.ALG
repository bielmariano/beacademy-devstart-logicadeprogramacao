Algoritmo "exercicio-12"

Var
// Seção de Declarações das variáveis
   numero, i : inteiro

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
   escreval("Informe um número para tabuada:")
   leia(numero)
   i <- 1

   enquanto i <= 10 faca
      escreval(numero, " *", i, " =", (numero * i))
      i <- i + 1
   fimenquanto

Fimalgoritmo