Algoritmo "exercicio-19"

Var
// Seção de Declarações das variáveis
   soma, i : inteiro

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
   i <- 1

   repita

         soma <- (soma + i)
         i <- (i + 1)
         escreva(i)

   ate (i = 101)

   escreval("Total da soma dos 100 primeiros números inteiros:", soma)


Fimalgoritmo